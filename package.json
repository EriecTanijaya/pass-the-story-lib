{
  "name": "pass-the-story-lib",
  "private": true,
  "type": "module",
  "scripts": {
    "start:dev": "vite dev --port 3000 --mode dev",
    "build": "vite build",
    "preview": "bun run build && vite preview",
    "test": "vitest run",
    "lint": "biome check --write --unsafe",
    "deploy": "bun run build --mode prod && wrangler secret bulk .env.prod && wrangler deploy",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "cf:typegen": "wrangler types",
    "tsc": "tsc",
    "auth-db:push": "bunx @better-auth/cli generate --config ./src/app/auth/lib/betterAuth/authServer.ts --output ./src/app/auth/lib/betterAuth/authTable.ts && bun db:push"
  },
